{
  "core": {
    "workingDir": ".reg",
    "actualDir": "__screenshots__",
    "thresholdRate": 0.001,
    "addIgnore": true,
    "ximgdiff": {
      "invocationType": "client"
    }
  },
  "plugins": {
    "reg-keygen-git-hash-plugin": true,
    "reg-notify-github-plugin": {
      "prComment": true,
      "prCommentBehavior": "default",
      "setCommitStatus": false,
      "clientId": "Mzc1sjAzMTIz0C8rKtE3MbM0NjYzM9YvSi1IzUsvzcwzMgIA",
      "shortDescription": true
    },
    "reg-publish-s3-plugin": {
      "bucketName": "repenguin22-vrt-sample",
      "pathPrefix": "reg-screenshots",
      "acl": "private",
      "sdkOptions": {
        "region": "ap-northeast-1"
      }
    }
  }
}
